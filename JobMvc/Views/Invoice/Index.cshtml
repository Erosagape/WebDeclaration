                        $('#tbDetails tbody tr.selected').removeClass('selected'); //ล้างทุก row ที่มีการ select ก่อน
                        $('#tbDetails').DataTable().page(idxPage).draw(false);

                        $('#tbDetails tbody tr.selected').removeClass('selected'); //ล้างทุก row ที่มีการ select ก่อน
