                    $('#' + strID).empty();
                    $('#' + strID).append($('<option>',{value:""}).text(""));
                    for(var i=0;i< CurrencyList.length;i++)
                    {
                        $('#' + strID).append($('<option>',{ value : CurrencyList[i].Code})
