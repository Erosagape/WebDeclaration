            function filterInv()
            {
                var ref = $('#selRefNo').val();
                var invFilter = $.grep(DocList, function(data) {
                    return data.RefNO === ref;
                });
                loadInvNo(invFilter);;
            }
            function loadData()
            {
                var inv = $('#selInvNo').val();
                var ref = $('#selRefNo').val();
                var invFilter = $.grep(DocList, function(data) {
                    return data.InvNO === inv && data.RefNO=== ref;
                });
                if(invFilter.length>0)
                {
                    loadHeader(invFilter[0]);
                }
            }
